{
    "extends": "tslint-config-airbnb",
    "rules": {
        "align": [
            true,
            "parameters"
        ],
        "no-unused-variable": false,
        "max-line-length": [
            true,
            140
        ],
        "ter-indent": 4,
        "ter-arrow-parens": false,
        "object-shorthand-properties-first": false,
        "import-name": false
    }
}
